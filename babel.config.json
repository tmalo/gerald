{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "api": "./src/front/graphql",
          "enums": "./src/enums",
          "components": "./src/front/components"
        },
        "extensions": [".js", ".jsx", ".graphql"]
      }
    ],
    "@babel/plugin-syntax-dynamic-import",
    "import-graphql"
  ],
  "presets": ["@babel/preset-env", "@babel/preset-react"]
}
