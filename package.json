{
  "name": "gerald",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "eslint -c ./src/.eslintrc ./src",
    "dev": "yarn server:dev & yarn start",
    "build": "yarn client:build && yarn server:build",
    "clean": "rm -rf build/ && rm -rf dist/",
    "server:dev": "cross-env NODE_ENV=development DISABLE_LOGGING=false keystone dev --entry src/server.config.js",
    "server:build": "cross-env NODE_ENV=production keystone build --entry src/server.config.js",
    "server:start": "cross-env NODE_ENV=production node src/server.js",
    "server:create-tables": "cross-env CREATE_TABLES=true keystone create-tables --entry src/server.js",
    "start": "cross-env PORT=5000 NODE_ENV=development webpack-dev-server --config ./webpack.dev.config.js --progress",
    "client:build": "cross-env NODE_ENV=production webpack --config ./webpack.prod.config.js --progress --mode production",
    "client:test": "jest",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "dependencies": {
    "@date-io/moment": "1.x",
    "@keystonejs/adapter-mongoose": "^8.1.3",
    "@keystonejs/app-admin-ui": "^7.0.2",
    "@keystonejs/app-graphql": "^5.1.8",
    "@keystonejs/app-static": "^5.1.2",
    "@keystonejs/auth-password": "^5.1.9",
    "@keystonejs/field-content": "^7.0.0",
    "@keystonejs/fields": "^12.0.1",
    "@keystonejs/fields-authed-relationship": "^1.0.6",
    "@keystonejs/keystone": "^11.0.0",
    "@keystonejs/list-plugins": "^7.0.0",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.10.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "apollo-boost": "^0.4.9",
    "ci-info": "^2.0.0",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "cross-env": "^7.0.0",
    "dev-null": "^0.1.1",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.6",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "forky": "^1.2.0",
    "formik": "^2.1.4",
    "graphql": "^15.1.0",
    "helmet": "^3.23.1",
    "history": "^4.10.1",
    "material-ui-formik-components": "^0.7.1",
    "moment": "^2.26.0",
    "mongoose": "^5.9.18",
    "node-sass": "^4.14.1",
    "ora": "^4.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-draft-wysiwyg": "^1.14.5",
    "react-inline-editing": "^1.0.10",
    "react-perfect-scrollbar": "^1.5.8",
    "react-router-dom": "^5.2.0",
    "validate.js": "^0.13.1"
  },
  "resolutions": {
    "**/**/http-proxy": "^1.18.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(s?css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/public/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/lists/"
    ]
  },
  "prettier": {
    "printWidth": 90,
    "tabWidth": 2,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "overrides": [
      {
        "files": "*.test.js",
        "options": {
          "semi": true
        }
      },
      {
        "files": [
          "*.html"
        ],
        "options": {
          "tabWidth": 4
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix -c ./src/.eslintrc"
    ],
    "*.+(js|jsx|json|css|md)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "http-proxy-middleware": "^1.0.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "react-dev-utils": "^10.2.1",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^5.1.3"
  }
}
